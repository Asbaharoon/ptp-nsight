<resource-manager-builder 
	xmlns="org.eclipse.ptp" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="org.eclipse.ptp http://eclipse.org/ptp/schemas/rm.xsd" 
	name="LSF-Generic-Batch">
	<control-data>
		<!-- Set the default value of this attribute if LSF is installed in a non-standard location -->
		<attribute name="bindir" visible="false"/>
		<!-- We set this to invisible because the values are not really modifiable -->
		<attribute name="queues" visible="false"/>
	</control-data>
	<monitor-data schedulerType="LSF">
	</monitor-data>
</resource-manager-builder>